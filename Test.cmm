 *テスト用に使う
 */

#include <stdio.hmm>
#include <stdlib.hmm>
#include <ctype.hmm>

#include "Assemble.hmm"
#include "Lexical.hmm"
#include "Pass1.hmm"
#include "OperationTbl.hmm"
#include "SymTbl.hmm"

public void tokenTest(){
    initLexical();

    int token_id = -1;
    getLabel();     //最初のラベル判定
    while(token_id != LxEOF){
        if(token_id == LxEOL){
            getLabel();
        }
        token_id = getToken();
        printf("%d\n",token_id);
    }
}
