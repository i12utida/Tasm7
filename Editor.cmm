public void printRsvWord(int i){
    printf("print %s\n",rsvWord[i].token);
    fputs(rsvWord[i].token,listfp);
    fputs("\t\t",listfp);
}

public void printBinary(int i){
    printf("print %s binary\n",rsvWord[i].token);
    fputs(operationtbl[i].machine_code,binaryfp);
}
